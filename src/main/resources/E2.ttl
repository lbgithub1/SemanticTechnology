@prefix ex: <http://airline.com/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#Classes
ex:Airline rdf:type rdfs:Class ;
           rdfs:comment "class which describes the airline" .
ex:Airport rdf:type rdfs:Class ;
           rdfs:comment "class which describes the airport" .
ex:Trip rdf:type rdfs:Class .
ex:FlightList rdf:type rdfs:Container .
ex:Flight rdf:type rdfs:Class .
ex:Customer rdf:type rdfs:Class .
ex:AirlineAirportOperation rdf:type rdfs:Class .
ex:FlightRoute rdf:type rdfs:Class .
ex:FlightTicket rdf:type rdfs:Class .

#Properties
ex:numberOfEmployees rdf:type rdf:Property .
ex:manager rdf:type rdf:Property .
ex:foundingYear rdf:type rdf:Property .
ex:flightDuration rdf:type rdf:Property .
ex:planeModel rdf:type rdf:Property .
ex:maxNumberOfPassengers rdf:type rdf:Property .
ex:orderTimestampTrip rdf:type rdf:Property .
ex:price rdf:type rdf:Property .
ex:customerName rdf:type rdf:Property .
ex:passportNumber rdf:type rdf:Property .
ex:seatClassification rdf:type rdf:Property .
ex:startTime rdf:type rdf:Property .
ex:endTime rdf:type rdf:Property .
ex:bookedBy rdf:type rdf:Property .
#ex:containsFlights rdf:type rdf:Property .
ex:seatIdentification rdf:type rdf:Property .
ex:hasStartedOperation rdf:type rdf:Property .
ex:atAirport rdf:type rdf:Property . #check ob richtig in neuer Datei
ex:inYear rdf:type rdf:Property . #Check ob wir das in neuem richtig haben
ex:flightStartAirport rdf:type rdf:Property .
ex:flightEndAirport rdf:type rdf:Property .
ex:hasFlightNumber rdf:type rdf:Property .
ex:containsRoute rdf:type rdf:Property .
ex:containsTicket rdf:type rdf:Property .
ex:responsibleAirline rdf:type rdf:Property .

#Domain and Ranges - No datatypes
ex:bookedBy rdfs:domain rdf:Trip ;
            rdfs:range ex:Customer .
ex:containsFlights rdfs:domain rdf:Trip ;
                   rdfs:range ex:FlightList .
#ex:consistsOf rdfs:domain ex:Flight  ;
#              rdfs:range ex:FlightList .
ex:hasStartedOperation rdfs:domain ex:Airline ;
                        rdfs:range ex:AirlineAirportOperation .
ex:atAirport rdfs:domain ex:AirlineAirportOperation ;
             rdfs:range ex:Airport .
ex:containsRoute rdfs:domain ex:Flight ;
                 rdfs:range ex:FlightRoute .
ex:containsTicket rdfs:domain ex:Flight ;
                  rdfs:range ex:FlightTicket .
#ex:responsibleAirline rdfs:domain ex:Trip ;
#                      rdfs:range ex:Airline .

#Domain and Ranges - XML datatypes
ex:numberOfEmployees rdfs:domain ex:Airline ;
                     rdfs:range xsd:integer .
ex:manager rdfs:domain ex:Airline ;
           rdfs:range xsd:string .
ex:foundingYear rdfs:domain ex:Airline ;
                rdfs:range xsd:integer .
ex:orderTimestampTrip rdfs:domain ex:Trip ;
                      rdfs:range xsd:dateTime .
ex:price rdfs:domain ex:Trip ;
         rdfs:range xsd:integer .
ex:customerName rdfs:domain ex:Customer ;
                rdfs:range xsd:string .
ex:passportNumber rdfs:domain ex:Customer ;
                  rdfs:range xsd:integer .
ex:startTime rdfs:domain ex:Trip, ex:Flight ;
             rdfs:range xsd:dateTime .
ex:endTime rdfs:domain ex:Trip, ex:Flight ;
           rdfs:range xsd:dateTime .
ex:seatClassification rdfs:domain ex:FlightTicket ;
                      rdfs:range xsd:string .
ex:flightDuration rdfs:domain ex:FlightRoute ;
                  rdfs:range xsd:integer .#float
ex:planeModel rdfs:domain ex:FlightRoute ;
              rdfs:range xsd:string .
ex:maxNumberOfPassengers rdfs:domain ex:FlightRoute ;
                         rdfs:range xsd:integer .
ex:flightID rdfs:domain ex:Flight ;
            rdfs:range xsd:integer .#neue Version weggelassen
ex:flightStartAirPort rdfs:domain ex:FlightRoute ;
                      rdfs:range ex:Airport .#neue Version weggelassen
ex:flightEndAirport rdfs:domain ex:FlightRoute ;
                    rdfs:range ex:Airport .#neue Version weggelassen
ex:seatIdentification rdfs:domain ex:FlightTicket ;
                      rdfs:range xsd:string .
ex:inYear rdfs:domain ex:AirlineAirportOperation ;
          rdfs:range xsd:date .
ex:hasFlightNumber rdfs:domain ex:FlightRoute ;
                   rdfs:range xsd:string .

#Instantiation
ex:Berlin rdf:type ex:Airport .
ex:NewYork rdf:type ex:Airport .
ex:Rome rdf:type ex:Airport .
ex:London rdf:type ex:Airport .

ex:AirTrier rdf:type ex:Airline ;
            ex:numberOfEmployees 450 ;
            ex:manager "CarstenSphor" ;
            ex:foundingYear 1995 ;
            ex:hasStartedOperation ex:BerlinOperation;
            ex:hasStartedOperation ex:NewYorkOperation;
            ex:hasStartedOperation ex:RomeOperation;
            ex:hasStartedOperation ex:LondonOperation .

ex:BerlinOperation rdf:type ex:AirlineAirportOperation ;
                   ex:atAirport ex:Berlin ;
                   ex:inYear "2002-09-24"^^xsd:date .

ex:NewYorkOperation rdf:type ex:AirlineAirportOperation ;
                    ex:atAirport ex:NewYork ;
                    ex:inYear "2005-04-11"^^xsd:date .

ex:RomeOperation rdf:type ex:AirlineAirportOperation ;
                 ex:atAirport ex:Rome ;
                 ex:inYear "2010-05-20"^^xsd:date .

ex:LondonOperation rdf:type ex:AirlineAirportOperation ;
                   ex:atAirport ex:London ;
                   ex:inYear "2007-01-05"^^xsd:date .

ex:AlfredAlbrecht rdf:type ex:Customer ;
                  ex:passportNumber 12345678 .
ex:BerndtBrandt rdf:type ex:Customer ;
                ex:passportNumber 10111213 .
ex:ChristophConrad rdf:type ex:Customer ;
                   ex:passportNumber 14151617 .

ex:RouteBerlinToLondon a ex:FlightRoute ;
                       ex:hasFlightNumber "AT0101" ;
                       ex:flightStartAirport ex:Berlin ;
                       ex:flightEndAirport ex:London ;
                       ex:flightDuration 2 ;
                       ex:planeModel "A320" ;
                       ex:maxNumberOfPassengers 180 .

ex:RouteBerlinToRome a ex:FlightRoute ;
                     ex:hasFlightNumber "AT0102" ;
                     ex:flightStartAirport ex:Berlin ;
                     ex:flightEndAirport ex:Rome  ;
                     ex:flightDuration 2 ;
                     ex:planeModel "A319" ;
                     ex:maxNumberOfPassengers 124 .

ex:RouteBerlinToNewYork a ex:FlightRoute ;
                        ex:hasFlightNumber "AT0103" ;
                        ex:flightStartAirport ex:Berlin ;
                        ex:flightEndAirport ex:NewYork ;
                        ex:flightDuration 9 ;
                        ex:planeModel "A380" ;
                        ex:maxNumberOfPassengers 850 .

ex:RouteLondonBerlin a ex:FlightRoute ;
                     ex:hasFlightNumber "AT0201" ;
                     ex:flightStartAirport ex:London ;
                     ex:flightEndAirport ex:Berlin ;
                     ex:flightDuration 2 ;
                     ex:planeModel "A340" ;
                     ex:maxNumberOfPassengers 297 .

ex:RouteLondonRome a ex:FlightRoute ;
                   ex:hasFlightNumber "AT0202" ;
                   ex:flightStartAirport ex:London ;
                   ex:flightEndAirport ex:Rome ;
                   ex:flightDuration 3 ;
                   ex:planeModel "A340" ;
                   ex:maxNumberOfPassengers 297 .

ex:RouteLondonNewYork a ex:FlightRoute ;
                      ex:hasFlightNumber "AT0204" ;
                      ex:flightStartAirport ex:London ;
                      ex:flightEndAirport ex:NewYork ;
                      ex:flightDuration 8 ;
                      ex:planeModel "A380" ;
                      ex:maxNumberOfPassengers 850 .

ex:RouteRomeBerlin a ex:FlightRoute ;
                   ex:hasFlightNumber "AT0301" ;
                   ex:flightStartAirport ex:Rome ;
                   ex:flightEndAirport ex:Berlin ;
                   ex:flightDuration 2 ;
                   ex:planeModel "A319" ;
                   ex:maxNumberOfPassengers 124 .

ex:RouteRomeLondon a ex:FlightRoute ;
                   ex:hasFlightNumber "AT0302" ;
                   ex:flightStartAirport ex:Rome ;
                   ex:flightEndAirport ex:London ;
                   ex:flightDuration 3 ;
                   ex:planeModel "A340" ;
                   ex:maxNumberOfPassengers 297 .

ex:RouteRomeNewYork a ex:FlightRoute ;
                    ex:hasFlightNumber "AT0303" ;
                    ex:flightStartAirport ex:Rome ;
                    ex:flightEndAirport ex:NewYork ;
                    ex:flightDuration 10 ;
                    ex:planeModel "A380" ;
                    ex:maxNumberOfPassengers 850 .

ex:RouteNewYorkBerlin a ex:FlightRoute ;
                      ex:hasFlightNumber "AT0401" ;
                      ex:flightStartAirport ex:NewYork ;
                      ex:flightEndAirport ex:Berlin ;
                      ex:flightDuration 9 ;
                      ex:planeModel "A380" ;
                      ex:maxNumberOfPassengers 850 .

ex:RouteNewYorkLondon a ex:FlightRoute ;
                      ex:hasFlightNumber "AT0402" ;
                      ex:flightStartAirport ex:NewYork ;
                      ex:flightEndAirport ex:London ;
                      ex:flightDuration 8 ;
                      ex:planeModel "A380" ;
                      ex:maxNumberOfPassengers 850 .

ex:RouteNewYorkRome a ex:FlightRoute ;
                    ex:hasFlightNumber "AT0403" ;
                    ex:flightStartAirport ex:NewYork ;
                    ex:flightEndAirport ex:Rome ;
                    ex:flightDuration 10 ;
                    ex:planeModel "A380" ;
                    ex:maxNumberOfPassengers 850 .

#Trip1
ex:Trip1 rdf:type ex:Trip ;
         ex:startTime "2024-05-01T00:00:00" ;
         ex:endTime "2024-05-01T022:30:00" ;
         ex:price 780 ;
         ex:orderTimestampTrip "2024-04-01T00:00:00" ;
         ex:bookedBy ex:AlfredAlbrecht ;
         ex:containsFlights ex:FlightList1 ;
         ex:responsibleAirline ex:AirTrier .

ex:FlightList1 rdf:type rdf:Seq ;
               rdf:_1 ex:Flight101 ;
               rdf:_2 ex:Flight102 ;
               rdf:_3 ex:Flight103 ;
               rdf:_4 ex:Flight104 .

ex:Flight101 ex:consistsOf ex:FirstDestination ;
             ex:flightID 1 ;
             ex:containsTicket ex:FlightTicket101 ;
             ex:containsRoute ex:RouteBerlinToLondon ;
             ex:startTime "2024-05-01T00:00:00" ;
             ex:endTime "2024-05-01T02:00:00" .

ex:Flight102 ex:consistsOf ex:SecondDestination ;
             ex:flightID 2 ;
             ex:containsTicket ex:FlightTicket102 ;
             ex:containsRoute ex:RouteLondonRome ;
             ex:startTime "2024-05-01T02:30:00" ;
             ex:endTime "2024-05-01T05:30:00" .

ex:Flight103 ex:consistsOf ex:ThirdDestination ;
             ex:flightID 3 ;
             ex:containsTicket ex:FlightTicket103 ;
             ex:containsRoute ex:RouteRomeNewYork ;
             ex:startTime "2024-05-01T03:00:00" ;
             ex:endTime "2024-05-01T13:00:00" .

ex:Flight104 ex:consistsOf ex:FourthDestination ;
             ex:flightID 4 ;
             ex:containsTicket ex:FlightTicket104 ;
             ex:containsRoute ex:RouteNewYorkBerlin ;
             ex:startTime "2024-05-01T13:30:00" ;
             ex:endTime "2024-05-01T22:30:00" .

ex:FlightTicket101 ex:seatClassification "Economy" ;
                   ex:seatIdentification "14C" .

ex:FlightTicket102 ex:seatClassification "Business" ;
                   ex:seatIdentification "1A" .

ex:FlightTicket103 ex:seatClassification "Economy" ;
                   ex:seatIdentification "23H" .

ex:FlightTicket104 ex:seatClassification "Economy" ;
                   ex:seatIdentification "26J" .


